    <link rel="stylesheet" href="/style/login.css">

</head>

<body>


    <%- include('./partials/header') %>


    <div class="login">
        <div class="login-triangle"></div>
        
        <h2 class="login-header">Entre</h2>
      
        <form class="login-container" method="POST" action="/login">
          <div class="errorLogin">
            <p>
              <% if(typeof errorLogin != "undefined") { %>
                <%= errorLogin %>
              <% } %>
            </p>
          </div>
          <p><input type="email" id="email"placeholder="Email" name="user_email"></p>
          <p><input type="password" id="password"placeholder="Senha" name="user_senha"></p>
          <p><input type="submit" value="Log in"></p>
        </form>
        <a href="/login/cadastro">NÃ£o tem conta? Cadastre-se</a>

     


      </div>


    <%- include('./partials/novoTesteFooter') %>


</body>

</html>